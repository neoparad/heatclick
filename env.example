# ClickInsight Pro 環境変数

# アプリケーション設定
NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development

# データベース設定
DATABASE_URL="postgresql://username:password@localhost:5432/clickinsight_pro"
CLICKHOUSE_URL="http://localhost:8123"
REDIS_URL="redis://localhost:6379"

# 認証設定
NEXTAUTH_SECRET=your-secret-key
NEXTAUTH_URL=http://localhost:3000

# 外部API設定
CLAUDE_API_KEY=your-claude-api-key
# Google Search Console API
GSC_CLIENT_EMAIL=your-service-account-email@project-id.iam.gserviceaccount.com
GSC_PRIVATE_KEY=-----BEGIN PRIVATE KEY-----\nYour Private Key\n-----END PRIVATE KEY-----
GSC_SITE_URL=sc-domain:example.com
# または
# GSC_SITE_URL=https://example.com
GA4_API_KEY=your-ga4-api-key
GOOGLE_ADS_API_KEY=your-google-ads-api-key

# メール設定
RESEND_API_KEY=your-resend-api-key
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_USER=your-email@example.com
SMTP_PASS=your-email-password

# 通知設定
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/your-webhook-url
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/your-webhook-url

# ファイルストレージ設定
UPLOADTHING_SECRET=your-uploadthing-secret
UPLOADTHING_APP_ID=your-uploadthing-app-id

# 監視・ログ設定
SENTRY_DSN=your-sentry-dsn
VERCEL_ANALYTICS_ID=your-vercel-analytics-id

# 開発設定
DEBUG=true
LOG_LEVEL=info

